#! /opt/homebrew/Cellar/icarus-verilog/12.0/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/system.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_sys.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_textio.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/v2005_math.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x12ff06b80 .scope module, "tb_dff" "tb_dff" 2 1;
 .timescale 0 0;
v0x12ff1c770_0 .var "clk", 0 0;
v0x12ff1c810_0 .var "d", 0 0;
v0x12ff1c8c0_0 .net "q", 0 0, v0x12ff1c5c0_0;  1 drivers
v0x12ff1c990_0 .var "rst", 0 0;
S_0x12ff06cf0 .scope module, "uut" "dff" 2 4, 3 1 0, S_0x12ff06b80;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "d";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "rst";
    .port_info 3 /OUTPUT 1 "q";
v0x12ff057c0_0 .net "clk", 0 0, v0x12ff1c770_0;  1 drivers
v0x12ff1c520_0 .net "d", 0 0, v0x12ff1c810_0;  1 drivers
v0x12ff1c5c0_0 .var "q", 0 0;
v0x12ff1c670_0 .net "rst", 0 0, v0x12ff1c990_0;  1 drivers
E_0x12ff06f90 .event posedge, v0x12ff1c670_0, v0x12ff057c0_0;
    .scope S_0x12ff06cf0;
T_0 ;
    %wait E_0x12ff06f90;
    %load/vec4 v0x12ff1c670_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x12ff1c5c0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x12ff1c520_0;
    %assign/vec4 v0x12ff1c5c0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x12ff06b80;
T_1 ;
    %delay 1, 0;
    %load/vec4 v0x12ff1c770_0;
    %inv;
    %store/vec4 v0x12ff1c770_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x12ff06b80;
T_2 ;
    %vpi_call 2 12 "$monitor", "Time=%0t | clk=%b rst=%b d=%b q=%b", $time, v0x12ff1c770_0, v0x12ff1c990_0, v0x12ff1c810_0, v0x12ff1c8c0_0 {0 0 0};
    %vpi_call 2 13 "$dumpfile", "sim/tb_dff.vcd" {0 0 0};
    %vpi_call 2 14 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x12ff06b80 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12ff1c770_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12ff1c810_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x12ff1c990_0, 0, 1;
    %delay 2, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12ff1c990_0, 0, 1;
    %delay 2, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x12ff1c810_0, 0, 1;
    %delay 2, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12ff1c810_0, 0, 1;
    %delay 4, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x12ff1c810_0, 0, 1;
    %delay 4, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x12ff1c990_0, 0, 1;
    %delay 2, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12ff1c990_0, 0, 1;
    %delay 4, 0;
    %vpi_call 2 24 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
