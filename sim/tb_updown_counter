S_0x1567052b0 .scope module, "tb_updown_counter" "tb_updown_counter" 2 1;
 .timescale 0 0;
v0x15671c8b0_0 .var "clk", 0 0;
v0x15671c950_0 .net "count", 3 0, v0x15671c650_0;  1 drivers
v0x15671ca00_0 .var "rst", 0 0;
v0x15671cad0_0 .var "updown", 0 0;
S_0x156706a20 .scope module, "uut" "updown_counter" 2 5, 3 1 0, S_0x1567052b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "updown";
    .port_info 3 /OUTPUT 4 "count";
v0x156705420_0 .net "clk", 0 0, v0x15671c8b0_0;  1 drivers
v0x15671c650_0 .var "count", 3 0;
v0x15671c700_0 .net "rst", 0 0, v0x15671ca00_0;  1 drivers
v0x15671c7b0_0 .net "updown", 0 0, v0x15671cad0_0;  1 drivers
E_0x156705b00 .event posedge, v0x15671c700_0, v0x156705420_0;
    .scope S_0x156706a20;
T_0 ;
    %wait E_0x156705b00;
    %load/vec4 v0x15671c700_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x15671c650_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x15671c7b0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x15671c650_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x15671c650_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x15671c650_0;
    %subi 1, 0, 4;
    %assign/vec4 v0x15671c650_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1567052b0;
T_1 ;
    %delay 1, 0;
    %load/vec4 v0x15671c8b0_0;
    %inv;
    %store/vec4 v0x15671c8b0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x1567052b0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x15671c8b0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x15671ca00_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x15671cad0_0, 0, 1;
    %vpi_call 2 14 "$monitor", "time:%t clk:%d rst:%d updown:%d count:%d", $time, v0x15671c8b0_0, v0x15671ca00_0, v0x15671cad0_0, v0x15671c950_0 {0 0 0};
    %vpi_call 2 15 "$dumpfile", "sim/tb_updown_counter.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x1567052b0 {0 0 0};
    %delay 2, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x15671ca00_0, 0, 1;
    %delay 35, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x15671cad0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x15671cad0_0, 0, 1;
    %delay 16, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x15671ca00_0, 0, 1;
    %delay 2, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x15671ca00_0, 0, 1;
    %delay 30, 0;
    %vpi_call 2 23 "$finish" {0 0 0};
    %end;
    .thread T_2;
