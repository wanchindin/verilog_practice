:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x14ef06210 .scope module, "tb_counter" "tb_counter" 2 1;
 .timescale 0 0;
v0x14ef1b2a0_0 .var "clk", 0 0;
v0x14ef1b340_0 .net "count", 3 0, v0x14ef1b0f0_0;  1 drivers
v0x14ef1b3f0_0 .var "rst", 0 0;
S_0x14ef04e40 .scope module, "uut" "counter" 2 5, 3 1 0, S_0x14ef06210;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 4 "count";
v0x14ef06380_0 .net "clk", 0 0, v0x14ef1b2a0_0;  1 drivers
v0x14ef1b0f0_0 .var "count", 3 0;
v0x14ef1b1a0_0 .net "rst", 0 0, v0x14ef1b3f0_0;  1 drivers
E_0x14ef0b530 .event posedge, v0x14ef1b1a0_0, v0x14ef06380_0;
    .scope S_0x14ef04e40;
T_0 ;
    %wait E_0x14ef0b530;
    %load/vec4 v0x14ef1b1a0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x14ef1b0f0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x14ef1b0f0_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x14ef1b0f0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x14ef06210;
T_1 ;
    %delay 1, 0;
    %load/vec4 v0x14ef1b2a0_0;
    %inv;
    %store/vec4 v0x14ef1b2a0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x14ef06210;
T_2 ;
    %vpi_call 2 13 "$monitor", "time:%t clk:%d rst:%d count:%d", $time, v0x14ef1b2a0_0, v0x14ef1b3f0_0, v0x14ef1b340_0 {0 0 0};
    %vpi_call 2 14 "$dumpfile", "sim/tb_counter.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x14ef06210 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14ef1b2a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x14ef1b3f0_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14ef1b3f0_0, 0, 1;
    %delay 35, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x14ef1b3f0_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x14ef1b3f0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 21 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
